{
  "name": "redstr",
  "version": "0.2.13",
  "description": "Native string transformations for security testing - WAF bypass, XSS, SQL injection, phishing, and evasion testing",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/arvid-berndtsson/redstr"
  },
  "homepage": "https://github.com/arvid-berndtsson/redstr",
  "bugs": {
    "url": "https://github.com/arvid-berndtsson/redstr/issues"
  },
  "license": "MIT",
  "keywords": [
    "security",
    "red-team",
    "obfuscation",
    "waf-bypass",
    "xss",
    "sql-injection",
    "penetration-testing",
    "napi-rs",
    "rust"
  ],
  "author": "Arvid Berndtsson <dev@arvid.tech>",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "redstr",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-musl"
      ]
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "node test.js",
    "universal": "napi universal",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "optionalDependencies": {
    "redstr-win32-x64-msvc": "0.2.13",
    "redstr-darwin-x64": "0.2.13",
    "redstr-linux-x64-gnu": "0.2.13",
    "redstr-linux-x64-musl": "0.2.13",
    "redstr-linux-arm64-gnu": "0.2.13",
    "redstr-darwin-arm64": "0.2.13",
    "redstr-linux-arm64-musl": "0.2.13"
  }
}
